{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"dynamodb:UpdateItem",
				"dynamodb:DescribeTable"
			],
			"Resource": "arn:aws:dynamodb:ap-southeast-1:068561046254:table/crypto-live-miniticker"
		},
		{
			"Effect": "Allow",
			"Action": [
				"s3:PutObject",
				"s3:AbortMultipartUpload"
			],
			"Resource": "arn:aws:s3:::crypto-live-bucket/batch_jsonl/*"
		},
		{
			"Effect": "Allow",
			"Action": ["secretsmanager:GetSecretValue"],
			"Resource": "arn:aws:secretsmanager:ap-southeast-1:068561046254:secret:crypto-live.binance_ws*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"ecr:GetAuthorizationToken",
				"ecr:BatchCheckLayerAvailability",
				"ecr:GetDownloadUrlForLayer",
				"ecr:BatchGetImage"
			],
			"Resource": "arn:aws:ecs:ap-southeast-1:068561046254:cluster/*"
		}
	]
}
