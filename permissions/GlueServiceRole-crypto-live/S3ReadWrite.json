{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:PutObject",
				"s3:PutObjectAcl",
				"s3:ListBucket",
				"s3:AbortMultipartUpload",
				"s3:PutObjectTagging"
			],
			"Resource": [
				"arn:aws:s3:::crypto-live-bucket",
				"arn:aws:s3:::crypto-live-bucket/*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"logs:CreateLogGroup",
				"logs:CreateLogStream",
				"logs:PutLogEvents"
			],
			"Resource": "*"
		}
	]
}